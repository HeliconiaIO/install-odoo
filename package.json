  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^22.7.4",
    "@types/prompts": "2.4.2",
    "@vercel/ncc": "0.38.1",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
